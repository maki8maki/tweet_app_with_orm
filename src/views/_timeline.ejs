<div data-test="posts-container">
    <% timeline.forEach(tweet => { %>
      <div class="posts-index-item" data-test="post-<%= tweet.post.id %>">
        <div class="post-left">
          <img src="<%= tweet.post.user.imageName %>">
        </div>
        <div class="post-right">
          <% if (tweet.type==="retweet") { %>
            <label data-test="post-label">
              <%= tweet.user.name %> Retweeted
            </label>
          <% } %>
          <div class="post-user-name">
            <a href="/users/<%= tweet.post.user.id %>" data-test="post-item-user">
              <%= tweet.post.user.name %>
            </a>
          </div>
          <a href="/posts/<%= tweet.post.id %>" data-test="post-item-content">
            <%= tweet.post.content %>
          </a>
        </div>
      </div>
      <% }) %>
  </div>
